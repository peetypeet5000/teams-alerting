{
  "name": "teams-alerting",
  "version": "1.0.0",
  "description": "A simple package to enable sending error objects to a teams chat as a webhook",
  "main": "dist/mjs/main.mjs",
  "types": "dist/types/main.d.ts",
  "module": "true",
  "dependencies": {
    "axios": "^1.3.4"
  },
  "exports": {
    "import": "./dist/mjs/main.mjs",
    "require": "./dist/cjs/main.cjs"
  },
  "devDependencies": {
    "@types/node": "^18.14.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "2.8.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "rm -rf dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && mv ./dist/cjs/main.js ./dist/cjs/main.cjs && mv ./dist/mjs/main.js ./dist/mjs/main.mjs",
    "lint": "npx eslint && npx prettier -w ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.sig.oregonstate.edu:ISCS/teams-alerting.git"
  },
  "author": "Peter LaMontagne",
  "license": "MIT"
}
